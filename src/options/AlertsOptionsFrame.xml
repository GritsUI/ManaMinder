    xsi:schemaLocation="http://www.blizzard.com/wow/ui/
    ..\..\..\FrameXML\UI.xsd">

  <Frame name="ManaMinder_Options_Alerts" parent="ManaMinder_Options" hidden="false" id="4">
    <Size>
      <AbsDimension x="566" y="383"/>
